# Google Maps API Configuration
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Database Configuration (for production deployment)
DATABASE_URL=postgresql://username:password@localhost:5432/queuecut_db
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=queuecut_db
POSTGRES_USER=queuecut_user
POSTGRES_PASSWORD=your_secure_password

# Authentication & Security
JWT_SECRET=your_jwt_secret_here
JWT_EXPIRE=7d
BCRYPT_ROUNDS=12

# API Configuration
API_BASE_URL=http://localhost:3000/api
PORT=3000

# Email & SMS Configuration (for notifications)
SMTP_HOST=smtp.gmail.com
SMTP_PORT=587
SMTP_USER=your_email@gmail.com
SMTP_PASSWORD=your_app_password
FROM_EMAIL=noreply@queuecut.com

TWILIO_ACCOUNT_SID=your_twilio_sid
TWILIO_AUTH_TOKEN=your_twilio_token
TWILIO_PHONE_NUMBER=+1234567890

# Analytics & Monitoring
ANALYTICS_ID=your_analytics_id

# Upload Configuration
CLOUDINARY_CLOUD_NAME=your_cloudinary_name
CLOUDINARY_API_KEY=your_cloudinary_key
CLOUDINARY_API_SECRET=your_cloudinary_secret

# Production Environment
NODE_ENV=development